						"id": "2481d107-0e0c-4545-8576-b04e4e2ff818",
						"exec": [
							"tests[\"Successful GET request\"] = responseCode.code === 200;",
							"if(responseCode.code === 200){",
							"    var responseData = JSON.parse(responseBody);",
							"    if(responseData.data.length === 0){",
							"        tests['Number Range detected'] = true === false;",
							"    }",
							"    else{",
							"        tests[\"Number Range Entity - Validate detected min_value\"] = responseData.data[0].entity_value.min_value === data.number_range_expected.min_value;",
							"        tests[\"Number Range Entity - Validate detected max_value\"] = responseData.data[0].entity_value.max_value === data.number_range_expected.max_value;",
							"        tests[\"Number Range Entity - Validate detected original text\"] = responseData.data[0].original_text === data.number_range_expected.original_text;",
							"    }",
							"}"
						],
						"type": "text/javascript"
					}
				}
			],
